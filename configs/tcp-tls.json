{
    "inbound": {
        "port": 443,
        "protocol": "vmess",
        "settings": {
			"udp": true,
            "clients": [
                {
                    "id": "UserUUID",
                    "alterId": 100
                }
            ]
        },
        "streamSettings": {
		    "network":"tcp",
            "security": "tls",
            "tlsSettings": {
                "serverName": "V2rayAddress",
                "certificates": [
                    {
                        "certificateFile": "/etc/v2ray/pem.pem",
                        "keyFile": "/etc/v2ray/key.key"
                    }
                ]
            }
        }
    },
    "outbound": {
        "protocol": "freedom",
        "settings": {}
    },
    "inboundDetour": [],
